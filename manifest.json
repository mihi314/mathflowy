{
  "name": "Mathflowy",
  "version": "0.1.0",
  "manifest_version": 2,

  "description": "Render TeX equations in Workflowy with Mathjax",
  "icons": {
        "128": "mathflowy-128.png",
        "48": "mathflowy-48.png",
        "16": "mathflowy-16.png"
   },

  "content_scripts": [ { "matches": ["https://workflowy.com/*"], "js": ["contentscript.js"], "run_at": "document_end" } ],

  "web_accessible_resources": [ "render.js" ]
}
